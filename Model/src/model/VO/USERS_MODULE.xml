<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="USERS_MODULE"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.VO.USERS_MODULE_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="GETROLEMASTERID"
    Kind="where"
    Type="java.math.BigDecimal">
    <TransientExpression
      Name="VariableScript"
      CodeSourceName="USERS_MODULE"/>
  </Variable>
  <SQLQuery><![CDATA[SELECT             
MM.MODULE_MENU_ID, MM.MODULE_MENU_NAME, MM.MODULE_MENU_LINK, MM.MODULE_MENU_DESC, MM.MODULE_MENU_ICON, MM.AST, 
RDM.ROLE_MASTER_ID 
FROM             
TBL_ROLE_DETAIL_MODULES RDM , TBL_MODULE_MENU MM           
Where             
MM.MODULE_MENU_ID = RDM.MODULE_MENU_ID            
AND            
RDM.R_VIEW = 'Y'            
AND           
MM.AST = 'Y'            
AND            
RDM.ROLE_MASTER_ID = :GETROLEMASTERID]]></SQLQuery>
  <ViewAttribute
    Name="ModuleMenuId"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="MODULE_MENU_ID"
    Expression="MODULE_MENU_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="ModuleMenuName"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="150"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MODULE_MENU_NAME"
    Expression="MODULE_MENU_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ModuleMenuLink"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="200"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MODULE_MENU_LINK"
    Expression="MODULE_MENU_LINK"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="200"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ModuleMenuDesc"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="200"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MODULE_MENU_DESC"
    Expression="MODULE_MENU_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="200"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ModuleMenuIcon"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="200"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MODULE_MENU_ICON"
    Expression="MODULE_MENU_ICON"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="200"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Ast"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="AST"
    Expression="AST"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="RoleMasterId"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="ROLE_MASTER_ID"
    Expression="ROLE_MASTER_ID"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="ModuleMenuId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
