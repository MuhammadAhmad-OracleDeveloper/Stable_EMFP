<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Course_Name_LOV"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  SelectList="TblCourse.AST, 
       TblCourse.COURSE_ID, 
       TblCourse.NAME"
  FromList="TBL_COURSE TblCourse"
  Where="AST='Y'"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Course_Name_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <EntityUsage
    Name="TblCourse"
    Entity="model.EO.TblCourse"/>
  <ViewAttribute
    Name="Ast"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblCourse"
    AliasName="AST"/>
  <ViewAttribute
    Name="CourseId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CourseId"
    EntityUsage="TblCourse"
    AliasName="COURSE_ID"/>
  <ViewAttribute
    Name="Name"
    IsUnique="true"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblCourse"
    AliasName="NAME"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
