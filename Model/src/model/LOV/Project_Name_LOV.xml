<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Project_Name_LOV"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  SelectList="TblProj.AST, 
       TblProj.NAME, 
       TblProj.PROJ_ID"
  FromList="TBL_PROJ TblProj"
  Where="AST='Y'"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Project_Name_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <EntityUsage
    Name="TblProj"
    Entity="model.EO.TblProj"/>
  <ViewAttribute
    Name="Ast"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblProj"
    AliasName="AST"/>
  <ViewAttribute
    Name="Name"
    IsUnique="true"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblProj"
    AliasName="NAME"/>
  <ViewAttribute
    Name="ProjId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ProjId"
    EntityUsage="TblProj"
    AliasName="PROJ_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
